{
  "_version": "1.76.0",
  "sap.app": {
    "id": "operations",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.18.6",
      "toolsId": "85c3c5a7-4428-4798-8015-843cbc4f8806"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/hmeltrading/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/mainService/metadata.xml",
          "odataVersion": "4.0"
        }
      },
       "s4HanaService": {
        "uri": "/sap/opu/odata/sap/ZHMELFACTMASTERTABLES_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/mainService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "operations-create": {
          "semanticObject": "operations",
          "action": "create",
          "title": "{{operations-create.flpTitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.140.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models":{
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "operations.i18n.i18n"
        }
      },
       "appModel": {
        "type": "sap.ui.model.json.JSONModel"
      },
       "s4HanaModel": {
        "type": "sap.ui.model.odata.v2.ODataModel",
        "dataSource": "s4HanaService",
        "preload": true,
        "settings": {
          "useBatch": false,
          "defaultBindingMode": "TwoWay",
          "defaultCountMode": "Inline"
        }
      },
      "oDataTradeEntry": {
        "dataSource": "mainService",
        "type": "sap.ui.model.odata.v4.ODataModel",
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true
        }
      },
    "": {
      "dataSource": "mainService",
      "preload": true,
       "type": "sap.ui.model.odata.v4.ODataModel",
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true
        }
    }
  },
  "resources": {
    "css": [
      {
        "uri": "css/style.css"
      }
    ]
  },
 "routing": {
  "config": {
    "routerClass": "sap.m.routing.Router",
    "controlAggregation": "pages",
    "controlId": "app",
    "transition": "slide",
    "type": "View",
    "viewType": "XML",
    "path": "operations.view",
    "async": true,
    "viewPath": "operations.view"
  },
  "routes": [
    {
      "name": "RouteTransferDefault",
      "pattern": "",
      "target": [
        "TargetTransferNew"
      ]
    },
    {
      "name": "RoutePaperTrade",
      "pattern": "PaperTrade/{tradeNumber}",
      "target": [
        "TargetPaperTrade"
      ]
    },
    {
      "name": "RoutePaperTradeNew",
      "pattern": "PaperTrade",
      "target": [
        "TargetPaperTradeNew"
      ]
    },
    {
      "name": "RouteTransfer",
      "pattern": "Transfer/{tradeNumber}",
      "target": [
        "TargetTransfer"
      ]
    },
    {
      "name": "RouteTransferNew",
      "pattern": "Transfer",
      "target": [
        "TargetTransferNew"
      ]
    }
  ],
  "targets": {
    "TargetPaperTrade": {
      "id": "PaperTrade",
      "name": "PaperTrade"
    },
    "TargetPaperTradeNew": {
      "id": "PaperTrade",
      "name": "PaperTrade"
    },
    "TargetTransfer": {
      "id": "Transfer",
      "name": "Transfer"
    },
    "TargetTransferNew": {
      "id": "Transfer",
      "name": "Transfer"
    }
  }
}
,
  "rootView": {
    "viewName": "operations.view.App",
    "type": "XML",
    "id": "App",
    "async": true
  }
},
"sap.cloud": {
  "public": true,
  "service": "operations"
}
}